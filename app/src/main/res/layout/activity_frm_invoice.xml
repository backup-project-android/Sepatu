<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Frm_invoice">

    <TextView
        android:id="@+id/textView13"
        android:layout_width="228dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView12"
        android:text="Rp10.000"
        android:textColor="#000000"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="154dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="95dp"
        android:text="Biaya Antar Jemput"
        android:textColor="#000000"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView42"
        android:layout_width="154dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="124dp"
        android:text="Grand Total"
        android:textColor="#000000"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="14dp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:textSize="8pt"
        android:text="Anda telah melakukan fast cleaning order"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="37dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView2"
        android:textAlignment="center"
        android:text="Total pembayaran sebesar" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="182dp"
        android:layout_marginEnd="0dp"
        android:text="Pembayaran dapat melalui rekening berikut"
        android:textAlignment="center" />

    <ImageView
        android:layout_width="45dp"
        android:layout_height="69dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="174dp"
        android:layout_toStartOf="@+id/textView7"
        android:src="@drawable/wallet" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="242dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="182dp"
        android:text="Mandiri" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="243dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/textView7"
        android:text="A.N : Susilo" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="242dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/textView8"
        android:text="73450986500" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView9"
        android:textAlignment="center"
        android:text="*Harga tertera sudah termasuk biaya antar-jemput" />

    <Button
        android:id="@+id/btnOk"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="176dp"
        android:background="#d67601"
        android:text="OK"
        android:textAllCaps="false"
        android:textColor="#fff"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="154dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="67dp"
        android:text="Total Pesanan"
        android:textColor="#000000"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/txt_grand"
        android:layout_width="228dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView42"
        android:text="Rp0"
        android:textColor="#000000"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/txt_totpesanan"
        android:layout_width="227dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView4"
        android:textColor="#000000"
        android:textStyle="bold"
        android:text="Rp0" />

    <TextView
        android:id="@+id/txt_kode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="139dp"
        android:textColor="#FFFFFF"
        android:text="TextView" />

    <TextView
        android:id="@+id/txt_grand2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="103dp"
        android:textColor="#FFFFFF"
        android:text="TextView" />
</RelativeLayout>